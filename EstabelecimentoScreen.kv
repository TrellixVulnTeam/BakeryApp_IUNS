<EstabelecimentoPage>:
    name: 'estabelecimento'
    MDToolbar:
        id: toolbar
        pos_hint: {"top": 1}
        elevation: 10
        title: "Bakery ABC - Estabelecimentos"
        md_bg_color: toolbarColor

    MDBottomNavigation:
        size_hint_y: .09
        panel_color: get_color_from_hex("#DF6710")
        text_color_active: get_color_from_hex("#fff4e6")
        first_widget: 'screen 3'
        
        MDBottomNavigationItem:
            name: 'screen 1'
            text: 'home'
            icon: 'home-circle'
            on_enter:
                root.manager.transition.direction = 'right'
                root.switchHome()

        MDBottomNavigationItem:
            name: 'screen 2'
            text: 'Estoque'
            icon: 'barcode'
            on_enter: root.switchEstoque()

        MDBottomNavigationItem:
            name: 'screen 3'
            text: 'Funcionários'
            icon: 'account-group'
            on_enter: root.switchFuncionario()

        MDBottomNavigationItem:
            name: 'screen 4'
            text: 'Vendas'
            icon: 'cash'
            on_enter: root.switchVendas()

    MDFillRoundFlatIconButton:
        text : 'Cadastrar estabelecimento'
        icon : "store-plus"
        pos_hint : {"center_x":.5,"center_y":.85}
        text_color : get_color_from_hex("#7E6B73")
        md_bg_color : get_color_from_hex("#FCE18E")
        font_size : 20
        user_font_size: "64sp"
        on_press:
            root.manager.transition.direction = 'left'
            root.switchCadastrar()

    MDFillRoundFlatIconButton:
        text : 'Alterar estabelecimento'
        icon : "cart-arrow-up"
        pos_hint : {"center_x":.5,"center_y":.77}
        text_color : get_color_from_hex("#7E6B73")
        md_bg_color : get_color_from_hex("#FCE18E")
        font_size : 20
        user_font_size: "64sp"
        on_press:
            root.manager.transition.direction = 'left'
            root.switchAlterar()

    MDFillRoundFlatIconButton:
        text : 'Consultar estabelecimentos'
        icon : "clipboard-text-search-outline"
        pos_hint : {"center_x":.5,"center_y":.69}
        text_color : get_color_from_hex("#7E6B73")
        md_bg_color : get_color_from_hex("#FCE18E")
        font_size : 20
        user_font_size: "64sp"
        on_press:
            root.manager.transition.direction = 'left'
            root.switchConsultar()

    MDFillRoundFlatIconButton:
        text : 'Remover estabelecimento'
